[
  {
    "key": "ctrl+alt+u",
    "command": "workbench.action.tasks.runTask",
    "args": "Docker Compose Up",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+d",
    "command": "workbench.action.tasks.runTask",
    "args": "Docker Compose Down",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+l",
    "command": "workbench.action.tasks.runTask",
    "args": "Docker Compose Logs",
    "when": "editorTextFocus"
  }
]
